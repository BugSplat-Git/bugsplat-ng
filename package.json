{
  "name": "my-angular-crasher",
  "version": "0.0.0-development",
  "database": "fred",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:lib && npm run build",
    "start": "http-server ./dist/my-angular-crasher",
    "build": "ng build my-angular-crasher",
    "build:app": "ng build my-angular-crasher --configuration=production --base-href=/my-angular-crasher/",
    "build:ci": "npm run build:lib && npm run build:app",
    "build:lib": "ng build bugsplat-ng --configuration=production",
    "postbuild": "npm run symbols dev",
    "postbuild:app": "npm run symbols",
    "postbuild:lib": "copyfiles README.md dist/bugsplat-ng",
    "test": "ng test",
    "lint": "ng lint",
    "release:lib": "npm run build:lib && cd dist/bugsplat-ng && npm publish",
    "symbols": "ts-node ./projects/my-angular-crasher/scripts/symbols.ts",
    "prepare": "husky install",
    "test:app": "ng test my-angular-crasher --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:lib": "ng test bugsplat-ng --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:ci": "npm run test:lib && npm run test:app",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/forms": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/router": "^14.0.4",
    "bugsplat": "^7.1.0",
    "core-js": "^2.6.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "14.0.0",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/schematics": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "^14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@angular/language-service": "^14.0.4",
    "@bugsplat/symbol-upload": "^4.1.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-teamcity-reporter": "^1.1.0",
    "ng-packagr": "^14.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-cli": "^5.2.1",
    "ts-node": "~8.0.2",
    "typescript": "~4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BugSplat-Git/bugsplat-ng.git"
  }
}
