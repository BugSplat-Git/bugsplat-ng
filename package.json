{
  "name": "my-angular-crasher",
  "version": "4.0.0",
  "database": "fred",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:lib && npm run build && npm run symbols dev",
    "start": "http-server ./dist/my-angular-crasher",
    "build": "ng build my-angular-crasher --configuration production",
    "build:lib": "ng build bugsplat-ng --configuration production && cp README.md dist/bugsplat-ng",
    "build:example": "ng build my-angular-crasher --configuration production --base-href my-angular-crasher",
    "postbuild": "npm run symbols dev",
    "postbuild:example": "npm run symbols",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release:lib": "npm run build:lib && cd dist/bugsplat-ng && npm publish",
    "symbols": "ts-node ./scripts/symbols.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/common": "^12.0.5",
    "@angular/compiler": "^12.0.5",
    "@angular/core": "^12.0.5",
    "@angular/forms": "^12.0.5",
    "@angular/platform-browser": "^12.0.5",
    "@angular/platform-browser-dynamic": "^12.0.5",
    "@angular/router": "^12.0.5",
    "bugsplat": "^7.1.0",
    "core-js": "^2.6.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^12.0.5",
    "@angular/compiler-cli": "^12.0.5",
    "@angular/language-service": "^12.0.5",
    "@bugsplat/symbol-upload": "^4.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.20.15",
    "codelyzer": "^5.1.2",
    "dotenv": "^10.0.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-teamcity-reporter": "^1.1.0",
    "ng-packagr": "^12.0.8",
    "protractor": "~7.0.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
