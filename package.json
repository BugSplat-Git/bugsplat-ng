{
  "name": "my-angular-crasher",
  "version": "0.0.0-development",
  "database": "fred",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:lib && npm run build",
    "start": "http-server ./dist/my-angular-crasher/browser",
    "build": "ng build my-angular-crasher --configuration=development",
    "build:app": "ng build my-angular-crasher --configuration=production --base-href=/my-angular-crasher/",
    "build:ci": "npm run build:lib && npm run build:app",
    "build:lib": "ng build bugsplat-ng --configuration=production",
    "postbuild": "npm run symbols",
    "postbuild:app": "npm run symbols",
    "postbuild:lib": "copyfiles README.md dist/bugsplat-ng",
    "test": "ng test",
    "lint": "ng lint",
    "release:lib": "npm run build:lib && cd dist/bugsplat-ng && npm publish",
    "symbols": "node -r dotenv/config ./node_modules/@bugsplat/symbol-upload/dist/bin/index.js -d ./dist/my-angular-crasher/browser",
    "prepare": "husky install",
    "pretest:app": "npm run build:lib",
    "pretest:lib": "npm run build:lib",
    "pretest:ci": "npm run build:lib",
    "test:app": "ng test my-angular-crasher --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:lib": "ng test bugsplat-ng --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:ci": "npm run test:lib && npm run test:app",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.10",
    "@angular/common": "^19.2.10",
    "@angular/compiler": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.10",
    "bugsplat": "^8.0.1",
    "core-js": "^3.39.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular-eslint/builder": "19.4.0",
    "@angular-eslint/eslint-plugin": "19.4.0",
    "@angular-eslint/eslint-plugin-template": "19.4.0",
    "@angular-eslint/schematics": "19.4.0",
    "@angular-eslint/template-parser": "19.4.0",
    "@angular/cli": "^19.2.11",
    "@angular/compiler-cli": "^19.2.10",
    "@angular/language-service": "^19.2.10",
    "@bugsplat/symbol-upload": "^10.1.4",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jasmine": "^5.1.5",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@typescript-eslint/types": "^8.32.0",
    "@typescript-eslint/utils": "^8.32.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jasmine-auto-spies": "^8.0.1",
    "jasmine-core": "^5.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-teamcity-reporter": "^1.1.0",
    "ng-packagr": "^19.0.1",
    "semantic-release": "^24.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BugSplat-Git/bugsplat-ng.git"
  }
}
