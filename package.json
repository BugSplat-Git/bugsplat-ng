{
  "name": "my-angular-crasher",
  "version": "5.0.0",
  "database": "fred",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:lib && npm run build && npm run symbols dev",
    "start": "http-server ./dist/my-angular-crasher",
    "build": "ng build my-angular-crasher --configuration production",
    "build:lib": "ng build bugsplat-ng --configuration production && cp README.md dist/bugsplat-ng",
    "build:example": "ng build my-angular-crasher --configuration production --base-href my-angular-crasher",
    "postbuild": "npm run symbols dev",
    "postbuild:example": "npm run symbols",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release:lib": "npm run build:lib && cd dist/bugsplat-ng && npm publish",
    "symbols": "ts-node ./scripts/symbols.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/router": "^13.2.2",
    "bugsplat": "^7.1.0",
    "core-js": "^2.6.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@bugsplat/symbol-upload": "^4.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.20.15",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-teamcity-reporter": "^1.1.0",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.0.2",
    "typescript": "~4.5.5"
  }
}
